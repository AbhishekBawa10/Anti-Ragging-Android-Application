<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/iam"
tools:context="MainActivity"
tools:ignore="Deprecated">
<EditText
android:id="@+id/editText2"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_x="60dp"
android:layout_y="210dp"
android:hint="Enter Phone Number"
android:phoneNumber="true"
android:textColorHint="#FFFFFF"
android:textColor="#FFFFFF"
android:textSize="20dp"
android:textStyle="bold"
tools:ignore="Deprecated" />
<EditText
android:id="@+id/editText3"
android:layout_width="289dp"
android:layout_height="92dp"
android:layout_x="60dp"
android:layout_y="285dp"
android:hint="Enter Emergency SMS"
android:textColor="#FFFFFF"
android:textSize="20dp"
android:textColorHint="#FFFFFF"
android:textStyle="bold" />
<ImageButton
android:id="@+id/btnSend"
android:layout_width="132dp"
android:layout_height="58dp"
android:layout_x="116dp"
android:layout_y="411dp"
android:background="@drawable/ui_button"
android:src="@drawable/ic_send_black_24dp"
android:text="Send Sms" />
<TextView
android:id="@+id/textView"
android:layout_width="wrap_content"
android:layout_height="43dp"
android:layout_x="12dp"
android:layout_y="115dp"
android:text="send emergency message "
android:textAllCaps="true"
android:textColor="#FFFFFF"
android:textSize="25dp"
android:textStyle="bold" />
<TextView
android:id="@+id/textView8"
  android:layout_width="wrap_content"
android:layout_height="45dp"
android:layout_x="0dp"
android:layout_y="506dp"
android:text="Emergency Number"
android:textAllCaps="true"
android:textColor="#FFFFFF"
android:textSize="22dp" />
<TextView
android:id="@+id/textView9"
android:layout_width="wrap_content"
android:layout_height="45dp"
android:layout_x="233dp"
android:layout_y="504dp"
android:text="8379******"
android:textColor="#FFFFFF"
android:textSize="22dp"
android:textStyle="bold" />
</AbsoluteLayout>
